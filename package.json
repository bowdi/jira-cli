{
  "name": "jira-cli",
  "description": "A JIRA Command Line Interface",
  "version": "2.3.0",
  "author": "Dan Shumaker <danpshumaker@gmail.com> (https://danshumaker.github.io/)",
  "contributors": [],
  "scripts": {
    "test": "jest",
    "test:e2e": "bin/tests.sh && exit 1",
    "patch-release": "npm version patch && npm publish && git push --follow-tags",
    "clean:dev": "rimraf -rf ./.jira-cli",
    "clean:global": "rm -rf ~/.jira-cli",
    "clean": "rimraf -rf ~/.jira-cli && npm uninstall jira-cli -g",
    "lint": "eslint . --ext .js,.ts --ignore-path .gitignore",
    "fix": "npm run lint -- --fix",
    "start": "npm run global",
    "dev": "node bin/jira.js",
    "global": "npm i -g && jira"
  },
  "dependencies": {
    "@types/jest": "^25.2.3",
    "alasql": "^0.4.11",
    "async": "^2.6.3",
    "cheerio": "^1.0.0-rc.3",
    "cli-table": "^0.2.0",
    "commander": "~1.1.1",
    "dargs": "^5.1.0",
    "fs": "0.0.1-security",
    "jira-connector": "^3.1.0",
    "nodemailer": "^6.4.14",
    "moment": "^2.29.1",
    "openurl": "^1.1.0",
    "qs": "^6.9.4",
    "superagent": "~3.7.0",
    "underscore": "^1.11.0"
  },
  "engines": {
    "node": ">= 0.8"
  },
  "keywords": [
    "jira",
    "api",
    "client",
    "command",
    "cli",
    "tool",
    "command-line-junky"
  ],
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git://github.com/danshumaker/jira-cli.git"
  },
  "bin": {
    "jira": "./bin/jira.js"
  },
  "husky": {
    "hooks": {
      "pre-push": "npm test"
    }
  },
  "devDependencies": {
    "ava": "^3.13.0",
    "eslint": "^6.8.0",
    "husky": "^4.3.0",
    "jest": "^25.3.0",
    "rimraf": "^3.0.2",
    "testr": "0.0.5",
    "jslint": "^0.12.1"
  }
}
